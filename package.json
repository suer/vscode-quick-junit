{
  "name": "vscode-quick-junit",
  "displayName": "Quick JUnit",
  "description": "Quick JUnit clone for Visual Studio Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "suer",
  "repository": {
    "type": "git",
    "url": "https://github.com/suer/vscode-quick-junit"
  },
  "extensionDependencies": [
    "vscjava.vscode-java-test"
  ],
  "icon": "images/icon.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-quick-junit.toggleTestFile",
        "title": "Toggle Test File"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-quick-junit.toggleTestFile",
        "key": "ctrl+9",
        "mac": "cmd+9",
        "when": "editorTextFocus"
      },
      {
        "command": "testing.runAtCursor",
        "key": "ctrl+0",
        "mac": "cmd+0",
        "when": "editorTextFocus"
      }
    ],
    "snippets": [
      {
        "language": "java",
        "path": "./snippets/java.json"
      }
    ]
  },
  
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "vsce": "^2.15.0"
  }
}
